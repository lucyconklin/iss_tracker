<div id="header"></div>
<div id="map"></div>
<div id="enter-zipcode"></div>
<div id="next-passover"></div>
<div id="send-text"></div>
<script src="http://d3js.org/d3.v3.min.js"></script>
<script src="http://d3js.org/topojson.v0.min.js"></script>
<script>
	var width = 1280,
	    height = 640;

	var svg = d3.select("#map").append("svg")
	    .attr("width", width)
	    .attr("height", height);

	var projection = d3.geo.equirectangular()
		.scale(203)
	    .translate([width/2,height/2])

	var path = d3.geo.path()
	    .projection(projection);

	var graticule = d3.geo.graticule();

	d3.json("https://gist.githubusercontent.com/abenrob/787723ca91772591b47e/raw/8a7f176072d508218e120773943b595c998991be/world-50m.json", function(error, world) {
	  svg.append("g")
	  	.attr("class", "land")
			.selectAll("path")
	  	.data([topojson.object(world, world.objects.land)])
		    .enter().append("path")
		    .attr("d", path);
		svg.append("g")
	  		.attr("class", "boundary")
			.selectAll("boundary")
	  		.data([topojson.object(world, world.objects.countries)])
		    .enter().append("path")
		    .attr("d", path);
	});
</script>
